<!-- //============================ table akun =============================== -->
<section class="section media-trading">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="section-heading">
          <h2>Akun Cents</h2>

        </div>
      </div>
    </div>

    <div class="row">
      <table class="table tb-media table-bordered table-striped table-light">
        <thead class="table-dark">
          <tr>
            <th scope="col mt-2">
              <h3>Akun <%= Akun4 %> </h3>
              <p class="text-light">Memiliki trading instrumen sebagai berikut : 40+ pasangan mata uang, 2 komoditas dan
                35 saham</p>
            </th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td class="mt-4">
              <% dataProstp.forEach(prostp => { %>
              <h5><%= prostp.instan %> </h5>
              <p class="text-muted"><%= prostp.detail_instan %> </p>
              <% }) %>
            </td>
          </tr>
        </tbody>
      </table>
      <a class="text-light font-weight-bold" style="padding-top: 10px; margin: auto !important;" href="#">Pahami Syarat
        Akun
        Trading
        lainnya ></a>
    </div>
  </div>
</section>
<!-- //============================ end table akun =============================== -->



